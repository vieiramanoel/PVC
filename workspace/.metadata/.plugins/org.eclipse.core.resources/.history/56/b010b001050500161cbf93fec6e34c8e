#include <opencv2/core/core.hpp>
#include <iostream>
#include <string>

#include <ReadData.h>


explicit ReadData::ReadData(int) : A(97), X(CV_PI), id("mydata1234"){

}
void ReadData::write(cv::FileStorage& fs) const{
        fs << "{" << "A" << A << "X" << X << "id" << id << "}";
}
void ReadData::read(const cv::FileNode& node){
        A = (int)node["A"];
        X = (double)node["X"];
        id = (std::string)node["id"];
}
